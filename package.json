{
  "name": "client",
  "version": "0.1.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "concurrently \"cd server && NODE_ENV=development node server.js\" \"cd client && npm start\"",
    "start-client": "cd client && NODE_ENV=development npm start",
    "start-server": "cd server && NODE_ENV=development nodemon server.js",
    "setup-client": "cd client && npm install",
    "setup-server": "cd server && npm install",
    "setup": "npm install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "concurrently": "^4.1.0"
  }
}
